# Auth Service - Docker Environment
# These values are used when running in Docker containers
# References to other services use Docker network hostnames

DATABASE_URL=postgres://postgres:postgres@postgres:5432/auth_service
JWT_SECRET=docker-dev-secret-change-in-production
JWT_EXPIRES_IN=15m
REFRESH_TOKEN_EXPIRY=7d
PORT=3001
SERVICE_NAME=auth-service
